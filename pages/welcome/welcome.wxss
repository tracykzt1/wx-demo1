page {
  height: 100%;
  background-color: #b3d4bd;
}

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  font-family: MicroSoft Yahei;
  font-weight: bold;
}
.face {
  width:200rpx; 
  height:200rpx;
  border-radius:50%;
  margin-top: 160rpx;
}

.title {
  font-size: 32rpx;
  margin-top: 100rpx;
}

.start-container {
  width: 200rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  border: 1px solid #fff;
  border-radius: 5px;
  margin-top: 200rpx;
}

.start {
  font-size: 22rpx;
}